{
  "name": "eslint-plugin-jsx-a11y",
  "version": "0.4.2",
  "description": "A static analysis linter of jsx and their accessibility with screen readers.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "a11y",
    "accessibility",
    "jsx"
  ],
  "author": "Ethan Cohen",
  "repository": {
    "type": "git",
    "url": "https://github.com/evcohen/eslint-plugin-jsx-a11y"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf lib && babel src --out-dir lib",
    "prepublish": "npm run lint && npm run test && npm run build",
    "coveralls": "cat ./reports/coverage/lcov.info | coveralls",
    "lint": "eslint  --config .eslintrc.js src tests",
    "pretest": "npm run lint",
    "test": "istanbul cover --dir reports/coverage node_modules/mocha/bin/_mocha tests/**/*.js -- --compilers js:babel-core/register --reporter nyan"
  },
  "devDependencies": {
    "babel-cli": "^6.6.0",
    "babel-core": "^6.6.0",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.6.0",
    "coveralls": "^2.11.8",
    "eslint": "2.2.x",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "MIT"
}
